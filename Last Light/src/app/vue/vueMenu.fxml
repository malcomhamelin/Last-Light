<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<Pane fx:id="panemenu" maxHeight="850.0" maxWidth="800.0" minHeight="512.0" minWidth="512.0" prefHeight="512.0" prefWidth="512.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controler.MenuControler">
   <children>
      <ImageView fitHeight="512.0" fitWidth="512.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../img/menu-background.jpeg" />
         </image>
      </ImageView>
      <Button fx:id="playButton" layoutX="150.0" layoutY="200.0" mnemonicParsing="false" onAction="#play" prefHeight="52.0" prefWidth="205.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="52.0" fitWidth="205.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../img/play-button.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="quitButton" layoutX="150.0" layoutY="300.0" mnemonicParsing="false" onAction="#quit" prefHeight="52.0" prefWidth="205.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="52.0" fitWidth="205.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../img/quit-button.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</Pane>
